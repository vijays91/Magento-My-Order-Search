<?xml version="1.0"?>
<config>
    <tabs>
        <myconf_vijaystore translate="label" module="myordersearch">
            <label>Vijay</label>
            <sort_order>100</sort_order>
        </myconf_vijaystore>
    </tabs>	
	
    <sections>
        <myordersearch_tab translate="label" module="myordersearch">
            <label>My Order Search</label>
            <tab>myconf_vijaystore</tab>
            <sort_order>1</sort_order>
            <show_in_default>1</show_in_default>
            <show_in_website>1</show_in_website>
            <show_in_store>1</show_in_store>
            <groups>
                <myordersearch_setting translate="label comment">
                    <label>My Order Search Setting</label>
                    <sort_order>503</sort_order>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>
                    <fields>
                      <myordersearch_active translate="label comment">
                            <label>My Order Search</label>
							<frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_enabledisable</source_model>
                            <sort_order>1</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </myordersearch_active>
                     </fields>
                </myordersearch_setting>		
            </groups>
        </myordersearch_tab>
    </sections>
</config>